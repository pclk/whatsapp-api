entrypoint = "main.py"
modules = ["python-3.10:v18-20230807-322e88b"]

[nix]
channel = "stable-23_05"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["sh", "-c", "fastapi run main.py"]
deploymentTarget = "cloudrun"
build = ["sh", "-c", "pip install fastapi openai"]

[[ports]]
localPort = 8000
externalPort = 80
exposeLocalhost = true
